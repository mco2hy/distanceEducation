@{ 
    Layout = "_Layout";
}
<h1>test</h1>
<a href=" @Url.Action("ManageClub","Club")">Kulüp Ekle</a>
<div id="Holder-Clubs"></div>
<script>
    $(document).ready(function () {
        $.ajax({
            type: "GET",
            url: "/club/getclubs",
            data: [],
            dataType: "json",
            contentType: "application/json; charset=utf-8;",
            success: function (result) {
                console.log(result);
                var html = "";

                for (var i = 0; i < result.length; i++) {
                    html += "<div>" + result[i].name + " - " + result[i].value + "</div>";
                }

                $("#Holder-Clubs").html(html);
            }
        });
    });
</script>